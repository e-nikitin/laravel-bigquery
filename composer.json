{
  "name": "e-nikitin/laravel-bigquery",
  "type": "library",
  "description": "Wrapper for Google BigQueryClient",
  "keywords": [
    "e-nikitin",
    "laravel-bigquery"
  ],
  "homepage": "https://github.com/e-nikitin/laravel-bigquery",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgenii Nikitin",
      "email": "mail.nikitin@mail.ru",
      "homepage": "https://github.com/e-nikitin",
      "role": "Developer"
    },
    {
      "name": "Aleksandr Kravchenko",
      "email": "googlogmob@gmail.com",
      "homepage": "https://github.com/smartmetrika",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~7.1",
    "google/cloud-bigquery": "^1.0.0",
    "illuminate/support": "^5.4",
    "madewithlove/illuminate-psr-cache-bridge": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "nikitin\\BigQuery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "nikitin\\BigQuery\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "nikitin\\BigQuery\\BigQueryServiceProvider"
      ],
      "aliases": {
        "BigQuery": "nikitin\\BigQuery\\BigQueryFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
